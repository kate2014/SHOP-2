        </fieldset>
        <fieldset>
            <lable>
                邮编：
            </lable>
            <input type="text"/>
        </fieldset>
        <fieldset>
            <lable>
                电话：
            </lable>
            <input type="text"/>
        </fieldset>
        <fieldset>
            <lable>
                手机：
            </lable>
            <input type="text"/>
        </fieldset>
        <fieldset>
            <lable>
                是否默认：
            </lable>
            <input type="checkbox"/>
        </fieldset>
        <fieldset>
            <lable>
                备注：
            </lable>
            <input type="text"/>
        </fieldset>
        <input type="button" value="确定"/>
        <input type="button" value="返回" src="发货点管理.html"/>
